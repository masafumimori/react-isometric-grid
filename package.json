{
  "name": "react-isometric-grid-ts",
  "version": "0.0.2",
  "description": "Isometric grid component for React and Typescript",
  "scripts": {
    "build": "yarn clean && cross-env NODE_ENV=production TARGET=umd webpack --bail",
    "build:demo": "yarn clean:demo && cross-env NODE_ENV=production TARGET=demo webpack --bail && yarn build-storybook",
    "clean": "rimraf dist",
    "clean:demo": "rimraf build",
    "start": "cross-env NODE_ENV=development TARGET=development webpack-dev-server --inline --hot",
    "lint": "eslint --ext .ts,.tsx ./src",
    "fix": "yarn format && yarn lint:fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"{src,examples}/**/*.{js,scss}\"",
    "prepublishOnly": "yarn lint && yarn test && yarn build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage && ./cc-test-reporter after-build --id=\"078550efafc3dbb8faf56159f96a1f6aed1bf6633c08cea85d7ac08a45c781b9\"",
    "deploy": "yarn build:demo && gh-pages -d build",
    "storybook": "cross-env TARGET=development start-storybook -p ${PORT:-3001} -h 0.0.0.0",
    "build-storybook": "cross-env NODE_ENV=production TARGET=demo build-storybook -o build/storybook"
  },
  "main": "dist/main.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/masafumimori/react-isometric-grid-ts"
  },
  "homepage": "https://github.com/masafumimori/react-isometric-grid-ts",
  "bugs": "https://github.com/masafumimori/react-isometric-grid-ts/issues",
  "authors": [
    "Masafumi Mori"
  ],
  "license": "MIT",
